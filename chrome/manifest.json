{
  "manifest_version": 2,
  "name": "Lucky Flix",
  "version": "0.0.1",
  "description": "Adds a Get Lucky button to Netflix.com, which plays a random film",
  "icons": {
    "32": "icon32.png",
    "64": "icon64.png",
    "110": "icon110.png"
  },
  "permissions": [],
  "content_scripts": [{
    "matches": ["*://*.netflix.com/WiHome"],
    "js": ["contentscript.js"],
    "run_at": "document_start"
  }],
  "web_accessible_resources": [
    "script.js",
    "jquery-1.11.1.min.js",
    "jquery.popupoverlay.js"
  ]
}