{
  "manifest_version": 2,
  "name": "Netflix - Get Lucky",
  "version": "1.0.0",
  "description": "Adds a Get Lucky button to netflix, which plays a random film",
  "permissions": [
  ],
  "content_scripts": [
    {
      "matches": ["*://*.netflix.com/WiHome"],
      "js" : ["contentscript.js"]
    }
  ],
  "web_accessible_resources": [
    "script.js"
  ]
}
